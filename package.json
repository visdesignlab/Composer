{
  "name": "phovea_workspace",
  "description": "helper package",
  "version": "0.0.1",
  "author": {
    "name": "Caleydo Team",
    "email": "contact@caleydo.org",
    "url": "https://caleydo.org"
  },
  "license": "BSD-3-Clause",
  "private": true,
  "engines": {
    "npm": ">= 3",
    "node": ">= 6",
    "iojs": ">= 3"
  },
  "scripts": {
    "compile": "tsc",
    "lint": "tslint -c tslint.json */src/**.ts */tests/**.ts",
    "prebuild": "npm run test",
    "pretest": "npm run compile",
    "test": "karma start",
    "posttest": "npm run lint",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "build:dev": "webpack",
    "build": "webpack --env prod",
    "watch": "webpack --watch",
    "docker:client_app": "cd client_app && npm run docker",
    "compile:client_app": "cd client_app && npm run compile",
    "lint:client_app": "cd client_app && npm run lint",
    "docs:client_app": "cd client_app && npm run docs",
    "test:client_app": "cd client_app && npm run test",
    "test:watch:client_app": "cd client_app && npm run test:watch",
    "build:dev:client_app": "cd client_app && npm run build:dev",
    "build:client_app": "cd client_app && npm run build",
    "start:client_app": "cd client_app && npm run start",
    "start:hot:client_app": "cd client_app && npm run start:hot",
    "watch:client_app": "cd client_app && npm run watch",
    "release:major:client_app": "cd client_app && npm run release:major",
    "release:minor:client_app": "cd client_app && npm run release:minor",
    "release:patch:client_app": "cd client_app && npm run release:patch",
    "dist:client_app": "cd client_app && npm run dist",
    "check:app_server": "./withinEnv \"exec 'cd app_server && npm run check'\"",
    "test:app_server": "./withinEnv \"exec 'cd app_server && npm run test'\"",
    "dist:app_server": "./withinEnv \"exec 'cd app_server && npm run dist'\"",
    "start:phovea_server": "./withinEnv \"python phovea_server --use_reloader --env dev api\"",
    "check:phovea_server": "./withinEnv \"exec 'cd phovea_server && npm run check'\"",
    "test:phovea_server": "./withinEnv \"exec 'cd phovea_server && npm run test'\"",
    "dist:phovea_server": "./withinEnv \"exec 'cd phovea_server && npm run dist'\""
  },
  "devDependencies": {
    "@types/jasmine": "2.5.41",
    "awesome-typescript-loader": "3.0.3",
    "css-loader": "0.26.1",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "extract-loader": "0.1.0",
    "file-loader": "0.10.0",
    "html-loader": "0.4.4",
    "imports-loader": "0.7.0",
    "jasmine": "2.5.3",
    "json-loader": "0.5.4",
    "karma": "1.4.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-firefox-launcher": "1.0.0",
    "karma-jasmine": "1.1.0",
    "karma-junit-reporter": "2.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "mkdirp": "0.5.1",
    "node-sass": "4.5.0",
    "null-loader": "0.1.1",
    "raw-loader": "0.5.1",
    "sass-loader": "5.0.0",
    "style-loader": "0.13.1",
    "tslib": "1.5.0",
    "tslint": "4.4.2",
    "typedoc": "0.5.5",
    "typescript": "2.2.0",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0"
  },
  "dependencies": {
    "phovea_core": "github:phovea/phovea_core#develop",
    "phovea_ui": "github:phovea/phovea_ui#v0.0.5",
    "@types/d3": "3.5.36",
    "d3": "3.5.17",
    "@types/d3-request": "^1.0.1",
    "@types/d3-selection": "^1.0.9",
    "@types/d3-array": "^1.0.7",
    "@types/d3-scale": "^1.0.4",
    "@types/d3-axis": "^1.0.6",
    "d3-request": "^1.0.3",
    "d3-selection": "^1.0.3",
    "d3-array": "^1.0.2",
    "d3-scale": "^1.0.3",
    "d3-axis": "^1.0.4",
    "@types/d3-collection": "^1.0.4",
    "d3-collection": "^1.0.2",
    "@types/d3-transition": "^1.0.7",
    "d3-transition": "^1.0.4",
    "@types/d3-shape": "^1.0.10",
    "d3-shape": "^1.0.6",
    "@types/d3-time-format": "^2.0.4",
    "d3-time-format": "^2.0.5"
  }
}
